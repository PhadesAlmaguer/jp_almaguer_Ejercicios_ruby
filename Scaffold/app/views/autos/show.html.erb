<div id="notice" class="row small-12 columns">
  <%= notice %>
</div>
<div class="row small-12 columns">
<div class="row">
  <div class="small-2 columns">
    <strong class="small-9 columns">Marca:</strong>
  </div>
  <div class="small-9 columns"><%= @auto.marca %></div>
</div>
  <div class="row">
    <div class="small-2 columns"><strong class="small-9 columns">Modelo:</strong></div>
    <div class="small-9 columns"><%= @auto.modelo %></div>
  </div>


  <div class="row">
    <div class="small-2 columns"><strong class="small-9 columns">Anio:</strong></div>
    <div class="small-9 columns"><%= @auto.anio %></div>
  </div>

  <div class="row">
    <div class="small-2 columns"><strong class="small-9 columns">Precio:</strong></div>
    <div class="small-9 columns"><%= @auto.precio %></div>
  </div>
  <div class="row">
    <div class="small-2 columns"><strong class="small-9 columns">Fecha adquisicion::</strong></div>
    <div class="small-9 columns"><%= @auto.fecha_adquisicion %></div>
  </div>

</div>
<div class="row small-12 columns">
  <div class="small-6 columns">
    <%= link_to 'Editar', edit_auto_path(@auto), class:'small-3 columns button alert left' %>
    <%= link_to 'Cancelar', autos_path, class:'small-3 columns button secondary left' %>
  </div>
</div>
