<%= form_for(auto) do |f| %>
  <% if auto.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(auto.errors.count, "error") %> prohibited this auto from being saved:</h2>

      <ul>
      <% auto.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class="">
  <div class="row">
    <div class="small-2 columns">
      <%= f.label :marca %>
    </div>
    <div class="small-10 columns">
      <%= f.text_field :marca %>
    </div>
  </div>

  <div class="row">
    <div class="small-2 columns"><%= f.label :modelo %></div>
    <div class="small-10 columns"><%= f.text_field :modelo %></div>
  </div>

  <div class="row">
    <div class="small-2 columns"><%= f.label :anio %></div>
    <div class="small-10 columns"><%= f.text_field :anio %></div>
  </div>

  <div class="row">
    <div class="small-2 columns"><%= f.label :precio %></div>
    <div class="small-10 columns"><%= f.text_field :precio %></div>
  </div>

  <div class="row">
    <div class="small-2 columns"><%= f.label :fecha_adquisicion %></div>
    <div class="small-10 columns"><%= f.date_select :fecha_adquisicion %></div>
  </div>
  <div class="small-12 columns">
    <div class="small-3 columns"><%= f.submit%> </div>
  </div>

</div>
<% end %>
